<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 4.0//EN"
        "http://www.tuckey.org/res/dtds/urlrewrite4.0.dtd">

<urlrewrite>

    <rule>
        <from>^/$</from>
        <to type="forward">/cds/index.html</to>
    </rule>

    <rule>
        <from>^/cds$</from>
        <to type="forward">/cds/index.html</to>
    </rule>

    <rule>
        <from>^/cds-web/</from>
        <to type="forward">/</to>
    </rule>

</urlrewrite>